em = allList[i];\r\n                        if (re.test(item.className)) {\r\n                            result.push(item);\r\n                        }\r\n                        ;\r\n                    }\r\n                    ;\r\n                }\r\n                ;\r\n                break;\r\n            // 获取标签名\r\n            default:\r\n                var oList = document.getElementsByTagName(selectorName);\r\n                for (var i = 0, len = oList.length; i < len; i++) {\r\n                    result.push(oList[i]);\r\n                }\r\n                ;\r\n                break;\r\n        }\r\n        ;\r\n        return result;\r\n    }\r\n};\r\nexport default element;\r\n//# sourceMappingURL=_element.js.map"}
